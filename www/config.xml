<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ionicframework.vsegda214536" version="4.0.9" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Такси Всегда</name>
    <description>
    Заказ такси
  </description>
    <author email="info@taxi21.ru" href="http://taxi-vsegda.ru/">
    Taxi21 Development Team
  </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="SplashScreenDelay" value="2000" />
    <preference name="FadeSplashScreenDuration" value="2000" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashScreen" value="screen" />
    <feature name="StatusBar">
        <param name="ios-package" onload="true" value="CDVStatusBar" />
    </feature>

    <platform name="ios">
      <icon src="res/iOS/Resources/icons/Icon.png"/>

      <icon height="57" src="res/iOS/Resources/icons/icon.png" width="57"/>
      <icon height="114" src="res/iOS/Resources/icons/icon@2x.png" width="114"/>
      <icon height="40" src="res/iOS/Resources/icons/icon-40.png" width="40"/>
      <icon height="80" src="res/iOS/Resources/icons/icon-40@2x.png" width="80"/>
      <icon height="120" src="res/iOS/Resources/icons/icon-40@3x.png" width="120"/>
      <icon height="50" src="res/iOS/Resources/icons/icon-50.png" width="50"/>
      <icon height="100" src="res/iOS/Resources/icons/icon-50@2x.png" width="100"/>
      <icon height="60" src="res/iOS/Resources/icons/icon-60.png" width="60"/>
      <icon height="120" src="res/iOS/Resources/icons/icon-60@2x.png" width="120"/>
      <icon height="180" src="res/iOS/Resources/icons/icon-60@3x.png" width="180"/>
      <icon height="72" src="res/iOS/Resources/icons/icon-72.png" width="72"/>
      <icon height="144" src="res/iOS/Resources/icons/icon-72@2x.png" width="144"/>
      <icon height="76" src="res/iOS/Resources/icons/icon-76.png" width="76"/>
      <icon height="152" src="res/iOS/Resources/icons/icon-76@2x.png" width="152"/>
      <icon height="167" src="res/iOS/Resources/icons/icon-83.5@2x.png" width="167"/>
      <icon height="29" src="res/iOS/Resources/icons/icon-small.png" width="29"/>
      <icon height="58" src="res/iOS/Resources/icons/icon-small@2x.png" width="58"/>
      <icon height="87" src="res/iOS/Resources/icons/icon-small@3x.png" width="87"/>

      <splash height="1136" src="res/iOS/Resources/splash/Default-568h@2x~iphone_640x1136.png" width="640"/>
      <splash height="1334" src="res/iOS/Resources/splash/Default-750@2x~iphone6-portrait_750x1334.png" width="750"/>
      <splash height="2208" src="res/iOS/Resources/splash/Default-1242@3x~iphone6s-portrait_1242x2208.png" width="1242"/>
      <splash height="1242" src="res/iOS/Resources/splash/Default-1242@3x~iphone6s-landscape_2208x1242.png" width="2208"/>
      <splash height="1536" src="res/iOS/Resources/splash/Default-Landscape@2x~ipad_2048x1536.png" width="2048"/>
      <splash height="768"  src="res/iOS/Resources/splash/Default-Landscape~ipad_1024x768.png" width="1024"/>
      <splash height="2048" src="res/iOS/Resources/splash/Default-Portrait@2x~ipad_1536x2048.png" width="1536"/>
      <splash height="1024" src="res/iOS/Resources/splash/Default-Portrait~ipad_768x1024.png" width="768"/>
      <splash height="960"  src="res/iOS/Resources/splash/Default@2x~iphone_640x960.png" width="640"/>
      <splash height="480"  src="res/iOS/Resources/splash/Default~iphone.png" width="320"/>

    </platform>

    <icon src="res/iOS/Resources/icons/Icon.png"/>

    <plugin name="cordova-plugin-file" spec="~4.3.0" />
    <plugin name="cordova-plugin-network-information" spec="~1.3.0" />
    <plugin name="cordova-plugin-appversion" spec="~1.0.0" />
    <plugin name="cordova-plugin-geolocation" spec="~2.3.0" />
    <plugin name="cordova-plugin-compat" spec="~1.0.0" />
    <plugin name="cordova-plugin-statusbar" spec="~2.1.3" />
    <plugin name="cordova-plugin-splashscreen" spec="~4.0.0" />
    <plugin name="cordova-plugin-console" spec="~1.0.3" />
    <plugin name="cordova-plugin-device" spec="~1.1.2" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.2" />
    <plugin name="cordova-plugin-media" spec="~2.4.0" />
    <plugin name="ionic-plugin-keyboard" spec="~2.2.1" />
    <plugin name="com.appmetrica.cordova.plugins.appmetrica" spec="https://github.com/slogger/AppMetricaCordovaPlugin.git" />
    <plugin name="cordova-plugin-x-toast" spec="~2.5.2" />
    <plugin name="cordova-plugin-datepicker" spec="~0.9.3" />
    <plugin name="cordova-plugin-crosswalk-webview" spec="~2.1.0">
        <variable name="XWALK_VERSION" value="21+" />
        <variable name="XWALK_LITEVERSION" value="xwalk_core_library_canary:17+" />
        <variable name="XWALK_COMMANDLINE" value="--disable-pull-to-refresh-effect" />
        <variable name="XWALK_MODE" value="embedded" />
        <variable name="XWALK_MULTIPLEAPK" value="true" />
    </plugin>
</widget>
